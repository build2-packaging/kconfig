test.arguments += -s

: alldefconfig
:
cat <<EOI >=Kconfig;
config FOO
	bool "Foo support"
	default y

config BAR
	bool "Bar support"
	default n

config BAZ
	bool "Baz support"
	depends on BAR
EOI
$* --alldefconfig Kconfig &.config;
cat .config >>EOO
#
# Automatically generated file; DO NOT EDIT.
# Main menu
#
CONFIG_FOO=y
# CONFIG_BAR is not set
EOO
